$Pattern Образец_прихода_клиента : event
$Relevant_resources
	_Парикмахерская: Парикмахерская Keep
$Body
_Парикмахерская
	Convert_event
	if( Парикмахерская.количество_в_очереди >= 2)
	{
		Образец_прихода_клиента.planning( time_now + Интервал_прихода( 50 ) );
		количество_в_очереди++;
	}
	else
	{
		Образец_прихода_клиента.planning( time_now + Интервал_прихода( 30 ) );
		количество_в_очереди++;
	}
$End
