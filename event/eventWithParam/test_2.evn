$Pattern Event_1: event trace
$Parameters Test_1 : integer 
$Relevant_resources
	_Resource : _Resource Keep
$Body
	_Resource
	Convert_event
		state = free;
$End

$Pattern Event_2: event trace
$Parameters Test_2 : integer = 2
$Relevant_resources
	_Resource: _Resource Keep
$Body
	_Resource
	Convert_event
		state = free;
		num = Test_2 + 1;
		Event_3.Planning(Time_now + Sequence_2(135));
$End


$Pattern Event_3: event trace
$Relevant_resources
	_Resource: _Resource Keep
$Body
	_Resource
	Convert_event
		Event_1.Planning(Time_now + Sequence_2(135), 3 );
		Event_2.Planning(Time_now + Sequence_1(135,45), * );
		Event_3.Planning(Time_now + Sequence_1(135,45));
$End



